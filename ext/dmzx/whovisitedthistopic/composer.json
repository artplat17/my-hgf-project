{
    "name": "dmzx/whovisitedthistopic",
    "type": "phpbb-extension",
    "description": "Shows the users who have visited a topic and the times they have visited.",
    "homepage": "http://www.dmzx-web.net",
    "version": "1.0.5",
    "time": "2016-12-26",
    "keywords": [
        "phpbb",
        "extension",
        "whovisitedthistopic"
    ],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "dmzx",
            "homepage": "http://www.dmzx-web.net",
            "email": "info@dmzx-web.net",
            "role": "Extension Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "phpbb/phpbb": ">=3.1.0,<3.3.0@dev"
    },
    "require-dev": {
        "phpbb/epv": "dev-master"
    },
    "extra": {
        "display-name": "Who Visited This Topic",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.0,<3.3.0@dev"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/who_visited_this_topic",
            "filename": "version_check"
        }
    }
}